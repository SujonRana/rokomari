                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-up-c"></i></p>';
                    } else if (response[i].ranking > response[i].previousRanking) {
                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-down-c"></i></p>';
                    } else if (response[i].ranking == response[i].previousRanking) {
                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-minus-round"></i></p>';
                    }
                    appendingHtml = appendingHtml + '</a>';
                }
                appendingHtml = appendingHtml + '<div class="text-center">'+
                    '<a href="/leader-board/company?filter=' + companyFilter + '" class="btn btn-view-all">View All</a>';
                company.html(appendingHtml);
            });
        }
    </script>

    <!-- Rating Pop-up -->
    
        <script type="module">
            setCookie('lastOrderPopUp', 'invalid', 7);
            deleteCookie('lastOrderPopUpShowed');
            function setCookie (name, value, expireDays) {
                var d = new Date();
                d.setTime(d.getTime() + (expireDays*24*60*60*1000));
                var expires = "expires="+ d.toUTCString();
                document.cookie = name + "=" + value + ";" + expires + ";path=/";
            }
            function deleteCookie (cookieName) {
                setCookie(cookieName, '', 0);
            }
        </script>
    

    



    






