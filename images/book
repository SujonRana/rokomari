            if (reviewerFilter != "ALL") {
                reviewerFilter = "ALL";
                loadReviewerLeaderBoard();
            }
        };
        reviewerTodayTab.onclick = function() {
            if (reviewerFilter != "TODAY") {
                reviewerFilter = "TODAY";
                loadReviewerLeaderBoard();
            }
        };
        function loadReviewerLeaderBoard () {
            const params = "type=REVIEWER" + "&filter=" + reviewerFilter;
            $.get("/leader-board/filter-wise", params, function(response) {
                let appendingHtml='';
                for (let i=0; i<response.length; i++) { 
                    appendingHtml = appendingHtml+'<div class="item-wrapper d-flex align-items-center">'+
                            '<p class="serial-number">'+parseInt(i+1)+'</p>'+
                            '<img src="/static/200/images/male.png" alt="reviewer" class="img-product rounded-circle img-fluid"/>'+
                            '<div class="product-info">'+
                            '<p class="product-name" title="'+response[i].title+'">'+response[i].title+'</p>'+
                            '</div>';
                    if (response[i].ranking < response[i].previousRanking) {
                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-up-c"></i></p>';
                    } else if (response[i].ranking > response[i].previousRanking) {
                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-down-c"></i></p>';
                    } else if (response[i].ranking == response[i].previousRanking) {
                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-minus-round"></i></p>';
                    }
                    appendingHtml = appendingHtml + '</div>';
                }
                appendingHtml = appendingHtml + '<div class="text-center">'+
                    '<a href="/leader-board/reviewer?filter=' + reviewerFilter + '" class="btn btn-view-all">View All</a>';
                reviewer.html(appendingHtml);
            });
        }

