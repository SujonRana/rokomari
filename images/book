        const book = $('.book-leader-board-data');
        let bookFilter = 'ALL';
        const bookAllTab = document.getElementById("book-all-tab");
        const bookTodayTab = document.getElementById("book-today-tab");
        const bookCategoryIdSelect = document.getElementById("bookCategoryId");
        if (bookFilter == 'ALL') {
            bookAllTab.classList.add("active");
        } else {
            bookTodayTab.classList.add("active");
        }
        bookCategoryIdSelect.onchange = function() {
            loadBookLeaderBoard();
        };
        bookAllTab.onclick = function() {
            if (bookFilter != "ALL") {
                bookFilter = "ALL";
                loadBookLeaderBoard();
            }
        };
        bookTodayTab.onclick = function() {
            if (bookFilter != "TODAY") {
                bookFilter = "TODAY";
                loadBookLeaderBoard();
            }
        };
        function loadBookLeaderBoard () {
            const categoryId = bookCategoryIdSelect.value;
            const params = "categoryId=" + categoryId+"&type=PRODUCT" + "&filter=" + bookFilter;
            $.get("/leader-board/category-wise", params, function(response) {
            let appendingHtml='';
            for (let i=0; i<response.length; i++) { 
                appendingHtml = appendingHtml+'<a class="item-wrapper d-flex align-items-center" target="_blank" href="/book/'+response[i].id+'">'+
                        '<p class="serial-number">'+parseInt(i+1)+'</p>'+
                        '<img src="https://ds.rokomari.store/rokomari110/ProductNew20190903/130X186/'+response[i].image+'" alt="product" class="img-product img-fluid"/>'+
                        '<div class="product-info">'+
                        '<p class="product-name" title="'+response[i].title+'">'+response[i].title+'</p>'+
                        '<p class="author-name">'+response[i].subtitle+'</p>'+
                        '</div>';
                if (response[i].ranking < response[i].previousRanking) {
                        appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-up-c"></i></p>';
                } else if (response[i].ranking > response[i].previousRanking) {
                        appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-down-c"></i></p>';
                } else if (response[i].ranking == response[i].previousRanking) {
                        appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-minus-round"></i></p>';
                }
                appendingHtml = appendingHtml + '</div>'+
                        '</a>';
            }
            appendingHtml = appendingHtml + '<div class="text-center">'+
                '<a href="/leader-board/product?categoryId=' + categoryId + '&filter=' + bookFilter + '" class="btn btn-view-all">View All</a>';
            book.html(appendingHtml);
            });
        }

        var author = $('.author-leader-board-data');
        let authorFilter = 'ALL';
        const authorAllTab = document.getElementById("author-all-tab");
        const authorTodayTab = document.getElementById("author-today-tab");
        const authorCategoryIdSelect = document.getElementById("authorCategoryId");
        if (authorFilter == 'ALL') {
            authorAllTab.classList.add("active");
        } else {
            authorTodayTab.classList.add("active");
        }
        authorCategoryId.onchange = function() {
            loadAuthorLeaderBoard();
        };
        authorAllTab.onclick = function() {
            if (authorFilter != "ALL") {
                authorFilter = "ALL";
                loadAuthorLeaderBoard();
            }
        };
        authorTodayTab.onclick = function() {
            if (authorFilter != "TODAY") {
                authorFilter = "TODAY";
                loadAuthorLeaderBoard();
            }
        };
        function loadAuthorLeaderBoard() {
            const categoryId = authorCategoryIdSelect.value;
            const params = "categoryId=" + categoryId+"&type=AUTHOR&filter=" + authorFilter;
            $.get("/leader-board/category-wise", params, function(response) {
                let appendingHtml = "";
                if (response.length > 0) {
                    appendingHtml = '<div class="top-three d-flex justify-content-center mb-3">';
                    let item2 = "";
                    let item3 = "";
                    let item1 = '<a class="author first"  title="'+response[0].title+'" target="_blank" href="/book/author/'+response[0].id+'">'+
                                    '<img src="/static/200/images/icon-winner-author.png" alt="winner" class="img-winner"/>'+
                                    '<img src="https://ds.rokomari.store/rokomari110/people/'+response[0].image+'" alt="author" class="img-fluid img-first"/>'+
                                    '<div class="badge">1</div>'+
                                    '<p class="author-name font-weight-bold text-center mt-3 ml-2">'+response[0].title+'</p>'+
                                '</a>';
                    if (response.length > 1) {
                        item2 = '<a class="author second"  title="'+response[1].title+'" target="_blank" href="/book/author/'+response[1].id+'">'+
                                    '<img src="https://ds.rokomari.store/rokomari110/people/'+response[1].image+'" alt="author" class="img-fluid"/>'+
                                    '<div class="badge">2</div>'+
                                    '<p class="author-name font-weight-bold mt-3">'+response[1].title+'</p>'+
                                '</a>';
                        if (response.length > 2) {
                            item3 = '<a class="author third"  title="'+response[2].title+'" target="_blank" href="/book/author/'+response[2].id+'">'+
                                        '<img src="https://ds.rokomari.store/rokomari110/people/'+response[2].image+'" alt="author" class="img-fluid"/>'+
                                        '<div class="badge">3</div>'+
                                        '<p class="author-name font-weight-bold mt-3">'+response[2].title+'</p>'+
                                    '</a>';
                        }
                    }
                    appendingHtml = appendingHtml + item2 + item1 + item3 + '</div>';
                    
                    if (response.length > 3) {
                        for (let i = 3; i<response.length; i++) {
                            appendingHtml = appendingHtml+'<a class="item-wrapper d-flex align-items-center"  title="'+response[i].title+'" target="_blank" href="/book/author/'+response[i].id+'">'+
                                    '<p class="serial-number">'+parseInt(i+1)+'</p>'+
                                    '<img src="https://ds.rokomari.store/rokomari110/people/'+response[i].image+'" alt="product" class="img-product rounded-circle img-fluid"/>'+
                                    '<div class="product-info">'+
                                    '<p class="product-name" title="'+response[i].title+'">'+response[i].title+'</p>'+
                                    '</div>';
                                    if (response[i].ranking < response[i].previousRanking) {
                                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-up-c"></i></p>';
                                    } else if (response[i].ranking > response[i].previousRanking) {
                                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-down-c"></i></p>';
                                    } else if (response[i].ranking == response[i].previousRanking) {
                                            appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-minus-round"></i></p>';
                                    }
                            appendingHtml = appendingHtml + '</div>'+
                                    '</a>';
                        }
                    }
                    appendingHtml = appendingHtml + '<div class="text-center">'+
                                        '<a href="/leader-board/author?categoryId=' + categoryId + '&filter=' + authorFilter + '" class="btn btn-view-all">View All</a>';
                }
                author.html(appendingHtml);
            });
        }


       const account = $('.account-leader-board-data');
       let accountFilter = 'ALL';
       const accountAllTab = document.getElementById("account-all-tab");
        const accountTodayTab = document.getElementById("account-today-tab");
        const accountTypeSelect = document.getElementById("leaderBoardAccountType");
        if (accountFilter == 'ALL') {
            accountAllTab.classList.add("active");
        } else {
            accountTodayTab.classList.add("active");
        }
        accountTypeSelect.onchange = function() {
            loadAccountLeaderBoard();
        };
        accountAllTab.onclick = function() {
            if (accountFilter != "ALL") {
                accountFilter = "ALL";
                loadAccountLeaderBoard();
            }
        };
        accountTodayTab.onclick = function() {
            if (accountFilter != "TODAY") {
                accountFilter = "TODAY";
                loadAccountLeaderBoard();
            }
        };
        function loadAccountLeaderBoard() {
            const accountType = accountTypeSelect.value;
            const params = "type=" + accountType + "&filter=" + accountFilter;
            $.get("/leader-board/account-type-wise", params, function(response) {
               let appendingHtml='';
               for (let i=0; i<response.length; i++) {
                   appendingHtml = appendingHtml+'<div class="item-wrapper d-flex align-items-center">'+
                           '<p class="serial-number">'+parseInt(i+1)+'</p>'+
                           '<img src="/static/200/images/male.png" alt="product" class="img-product rounded-circle img-fluid"/>'+
                           '<div class="product-info">'+
                           '<p class="product-name" title="'+response[i].title+'">'+response[i].title+'</p>'+
                           '<p class="phone-number">Mob: 01 ... '+response[i].subtitle.substring(7, 11)+'</p>'+
                           '</div>';
                           if (response[i].ranking < response[i].previousRanking) {
                                   appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-up-c"></i></p>';
                           } else if (response[i].ranking > response[i].previousRanking) {
                                   appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-arrow-down-c"></i></p>';
                           } else if (response[i].ranking == response[i].previousRanking) {
                                   appendingHtml = appendingHtml + '<p class="rank-icon"><i class="ion-minus-round"></i></p>';
                           }
                   appendingHtml = appendingHtml + '</div>';
               }
               appendingHtml = appendingHtml + '<div class="text-center">'+
                       '<a href="/leader-board/account?type=' + accountType + '&filter=' + accountFilter + '" class="btn btn-view-all">View All</a></div>';
               account.html(appendingHtml);
            });
        }

        const reviewer = $('.reviewer-leader-board-data');
        let reviewerFilter = 'ALL';
        const reviewerAllTab = document.getElementById("reviewer-all-tab");
        const reviewerTodayTab = document.getElementById("reviewer-today-tab");
